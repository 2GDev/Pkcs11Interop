{
  "title": "Pkcs11Interop",
  "copyright": "Copyright 2012-2017 The Pkcs11Interop Project",
  "description": "Managed .NET wrapper for unmanaged PKCS#11 libraries",
  "version": "3.3.0",
  "frameworks": {
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "COREFX" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "compile": [
      "../../../Pkcs11Interop/Pkcs11Interop/Common/**/*.cs",
      "../../../Pkcs11Interop/Pkcs11Interop/HighLevelAPI/**/*.cs",
      "../../../Pkcs11Interop/Pkcs11Interop/HighLevelAPI40/**/*.cs",
      "../../../Pkcs11Interop/Pkcs11Interop/HighLevelAPI41/**/*.cs",
      "../../../Pkcs11Interop/Pkcs11Interop/HighLevelAPI80/**/*.cs",
      "../../../Pkcs11Interop/Pkcs11Interop/HighLevelAPI81/**/*.cs",
      "../../../Pkcs11Interop/Pkcs11Interop/LowLevelAPI40/**/*.cs",
      "../../../Pkcs11Interop/Pkcs11Interop/LowLevelAPI41/**/*.cs",
      "../../../Pkcs11Interop/Pkcs11Interop/LowLevelAPI80/**/*.cs",
      "../../../Pkcs11Interop/Pkcs11Interop/LowLevelAPI81/**/*.cs"
    ]
  }
}
