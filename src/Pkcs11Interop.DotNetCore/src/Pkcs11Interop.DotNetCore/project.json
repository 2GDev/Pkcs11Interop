{
  "title": "Pkcs11Interop",
  "copyright": "Copyright 2012-2016 The Pkcs11Interop Project",
  "description": "Managed .NET wrapper for unmanaged PKCS#11 libraries",
  "version": "3.1.0",
  "dependencies": {
    "NETStandard.Library": "1.5.0-rc2-24027"
  },
  "frameworks": {
    "netstandard1.5": {
      "imports": "dnxcore50"
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "compile": [
      "../../../Pkcs11Interop/Common/**/*.cs",
      "../../../Pkcs11Interop/HighLevelAPI/**/*.cs",
      "../../../Pkcs11Interop/HighLevelAPI40/**/*.cs",
      "../../../Pkcs11Interop/HighLevelAPI41/**/*.cs",
      "../../../Pkcs11Interop/HighLevelAPI80/**/*.cs",
      "../../../Pkcs11Interop/HighLevelAPI81/**/*.cs",
      "../../../Pkcs11Interop/LowLevelAPI40/**/*.cs",
      "../../../Pkcs11Interop/LowLevelAPI41/**/*.cs",
      "../../../Pkcs11Interop/LowLevelAPI80/**/*.cs",
      "../../../Pkcs11Interop/LowLevelAPI81/**/*.cs"
    ],
    "define": [ "COREFX" ]
  }
}
