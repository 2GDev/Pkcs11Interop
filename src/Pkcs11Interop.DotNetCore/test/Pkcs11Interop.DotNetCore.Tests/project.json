{
  "title": "Pkcs11Interop.DotNetCore.Tests",
  "copyright": "Copyright 2012-2016 The Pkcs11Interop Project",
  "description": "Managed .NET wrapper for unmanaged PKCS#11 libraries",
  "version": "3.1.0",
  "dependencies": {
    "NETStandard.Library": "1.5.0-rc2-24027",
    "Pkcs11Interop.DotNetCore": {
      "target": "project"
    },
    "NUnit": "3.2.1",
    "dotnet-test-nunit": "3.4.0-alpha-2"
  },
  "testRunner": "nunit",
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "netcoreapp1.0",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0-rc2-3002702"
        }
      }
    }
  },
  "buildOptions": {
    "compile": [
      "../../../Pkcs11InteropTests/Common/**/*.cs",
      "../../../Pkcs11InteropTests/HighLevelAPI/**/*.cs",
      "../../../Pkcs11InteropTests/HighLevelAPI40/**/*.cs",
      "../../../Pkcs11InteropTests/HighLevelAPI41/**/*.cs",
      "../../../Pkcs11InteropTests/HighLevelAPI80/**/*.cs",
      "../../../Pkcs11InteropTests/HighLevelAPI81/**/*.cs",
      "../../../Pkcs11InteropTests/LowLevelAPI40/**/*.cs",
      "../../../Pkcs11InteropTests/LowLevelAPI41/**/*.cs",
      "../../../Pkcs11InteropTests/LowLevelAPI80/**/*.cs",
      "../../../Pkcs11InteropTests/LowLevelAPI81/**/*.cs",
      "../../../Pkcs11InteropTests/Settings.cs"
    ],
    "define": [ "COREFX" ]
  }
}
